package com.encinas;

import static org.junit.Assert.*;

import org.junit.Test;

public class Apptest {

	@Test
	public void dameElValorDelSimbolo() {
		String symbols = "-123456789X/";
		App ok = new App();
		for (int i = 0; i < symbols.length(); i++)
		assertEquals(10, ok.getvalueStrike("X"));
	}
	
}
