package com.encinas;

import static org.junit.Assert.*;

import org.junit.Test;

public class Apptest {

	@Test
	public void dameElValorDelSimbolo() {
		String symbols = "-123456789X/";
		App ok = new App();
		for (int i = 0; i < symbols.length(); i++)
		assertEquals(i, ok.getvalueStrike(symbols.charAt(i)));
		
		
	}
	@Test
	public void dameelvalor() {
		String symbols = "12345123451234512345";
		App ok = new App();
		int total = 60;
		for (int i = 0 ; i < symbols.length() ; i++)
		assertEquals(total, ok.getvalueStrike(symbols));
	}
	
}
