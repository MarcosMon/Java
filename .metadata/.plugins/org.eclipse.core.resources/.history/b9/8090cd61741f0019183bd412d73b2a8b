package org.lasencinas.bowlingGame;

public class ScoreCard {
	
	
	public static symbols = "-123456789X/";
	public ScoreCard() {
		
	}
	
	public int getvalueStrike(String cadena) {
		int total = 0;
		for (int i = 0; i < cadena.length(); i++) {
			total+=symbols.indexOf(cadena.charAt(i));
	
		}
		return total;
	}
}
