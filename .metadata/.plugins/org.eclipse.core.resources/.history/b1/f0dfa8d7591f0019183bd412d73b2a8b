package com.encinas;

public class Prueba {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String vater = "-123456789X";
		String cadena =  "123456788";
		int total = 0;
		for (int i = 0; i < cadena.length(); i++) {
			total+=vater.indexOf(cadena.charAt(i));
		}
		return total;
	}

}
