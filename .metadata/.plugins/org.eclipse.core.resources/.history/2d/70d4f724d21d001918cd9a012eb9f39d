package tarjetaUsuario;

public class TarjetaUsuario {
	private String id;
	private boolean activa;
	public TarjetaUsuario(String id, boolean activa) {
		this.id = id;
		this.activa = activa;
	}
	public void setActiva(boolean activa) {
		this.activa = activa;
	}
	
	public boolean getActiva(){
		return this.activa;
	}
}
